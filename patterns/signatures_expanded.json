  "contextual_rules": {
    "description": "Regras contextuais para evitar falsos positivos",
    "rules": [
      {
        "name": "preserve_oab_in_signature",
        "description": "Preservar linha com OAB se vier depois de nome de advogado",
        "pattern": "[A-Z][a-z]+(?:\\s+[A-Z][a-z]+)+\\n.*OAB",
        "action": "keep"
      },
      {
        "name": "remove_isolated_oab",
        "description": "Remover OAB isolada sem contexto",
        "pattern": "^OAB\\/[A-Z]{2}\\s+\\d+$",
        "action": "remove"
      },
      {
        "name": "preserve_legal_citation",
        "description": "Preservar citações de artigos de lei",
        "pattern": "(art(igo)?\\.?|lei)\\s*(nº|n\\.)?\\s*\\d+",
        "action": "keep"
      },
      {
        "name": "remove_page_numbers",
        "description": "Remover numeração de página isolada",
        "pattern": "^(Pág|Página|Fl|Folha)\\.?\\s*\\d+",
        "action": "remove"
      }
    ]
  }
}
