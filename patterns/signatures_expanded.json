  "regex_patterns": [
    "^[a-fA-F0-9]{32,}$",
    "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$",
    "\\b[A-Z]{2,}:[a-fA-F0-9]{40,}\\b",
    "\\bOAB\\/[A-Z]{2}\\s+\\d+\\b",
    "\\bCPF:\\s*\\d{3}\\.\\d{3}\\.\\d{3}-\\d{2}\\b",
    "\\bCNPJ:\\s*\\d{2}\\.\\d{3}\\.\\d{3}\\/\\d{4}-\\d{2}\\b",
    "\\d{7}-\\d{2}\\.\\d{4}\\. \\d{1}\\.\\d{2}\\.\\d{4}",
    "\\bAC\\s+[A-Z]+\\s+v\\d+\\b",
    "Pág\\.?\\s*\\d+\\s*(de|\\/)\\s*\\d+",
    "\\d{2}\\/\\d{2}\\/\\d{4}\\s+\\d{2}:\\d{2}(:\\d{2})?"
  ],

  "whitelist": {
    "description": "Termos que NUNCA devem ser removidos, mesmo em modo agressivo",
    "terms": [
